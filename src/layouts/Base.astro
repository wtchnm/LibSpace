---
import '../global.css'
---

<!doctype html>
<html lang='en'>
	<head>
		<link
			rel='preload'
			href='/Inter.woff2'
			as='font'
			type='font/woff2'
			crossorigin
		/>

		<meta charset='UTF-8' />
		<link rel='icon' type='image/svg+xml' href='/favicon.svg' />
		<meta name='viewport' content='width=device-width, initial-scale=1' />
		<meta name='generator' content={Astro.generator} />
		<meta name='description' content='Lastro' />
		<title>Shelf</title>
		<style is:inline>
		  @font-face {
			  font-family: "Inter fallback";
			  size-adjust: 106.95000000000003%;
				ascent-override: 110%;
				font-display: swap;
			  src: local("Arial");
			}
			@font-face {
				font-family: Inter;
				font-style: normal;
				font-weight: 100 900;
				font-display: swap;
				src: url('/Inter.woff2') format('woff2');
			}
		</style>
	</head>
	<body>
		<slot />
	</body>
</html>
